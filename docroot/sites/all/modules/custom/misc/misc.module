<?php

/*
 * implements hook_menu
 */
function misc_menu() {
	module_load_include('inc', 'pathauto');

	$customnodes = array('news');

	foreach ($customnodes as $key)
		$items[$key] = array(
			'page callback' => 'theme_render_template',
			'page arguments' => array(path_to_theme() . '/templates/node-unformatted--' . pathauto_cleanstring($key) . '.tpl.php'),
			'access callback' => TRUE,
		);

	return $items;	
}